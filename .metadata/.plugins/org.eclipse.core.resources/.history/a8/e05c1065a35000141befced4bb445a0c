
import java.util.Scanner;

public class ACM {

    public static void main(String[] args) {
       Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        int m=sc.nextInt();
        int [] a=new int[n];
        
        for(int j=0;j<n;j++){
           {
                a[j]=sc.nextInt(2);
            }
                }
        
         for(int j=0;j<n-1;j++){
             int max=0;int flag=0;
            for(int i=j+1;i<n;i++){
                int temp=and(a[j],a[i]);
                if(temp>max){
                    max=temp;
                    flag=1;
                }else if(temp==max){
                    flag++;
                }
            }
                }
        
    }
    private static int and(int a,int b){
        int flag=0;
        for(int i=0;i<a.length;i++){
            if((a[i]|b[i])==1){
                flag++;
            }
        }return flag;
    }
    
    private static int getBit(int n, int k) {
        return (n >> k) & 1;
    }
}
