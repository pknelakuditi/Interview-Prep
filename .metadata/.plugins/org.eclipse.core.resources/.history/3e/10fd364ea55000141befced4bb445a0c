	public static int and(int a, int b, int m) {
		int flag = 0;
		int temp=0;
		for (int i = 0; i < m; i++) {
			System.out.println((a%10)|(b%10)+" s");
			if (((a%10)|(b%10))==1) {
				flag++;
				a=a/10;
				b=b/10;
			}
		}
		return flag;
	}

	private static int getBit(int n, int m) {
		return (n >> m) & 1;
	}
}
