	public static long pow() {
		long m=1000000007;
		long t=1;
		long temp=1;
		for(long i=0;i<10000007;i++){
			temp=(temp*2);
			if(temp%m<100 && temp/m>1){
				t=i;
				break;
			}
		}
		System.out.println(temp%m);
		//return temp%m;
		return t;
	}
