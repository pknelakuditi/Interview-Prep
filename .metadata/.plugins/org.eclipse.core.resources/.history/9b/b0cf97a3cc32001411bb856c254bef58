package algos.standard.sorting;

public class Search {
    private int [] a;
	private int b;
	Search(int[] a ,int b){this.a=a;this.b=b;}
	
	public int binarysearch(int[] arr){
		int arr_len=arr.length;
		int mid=arr_len/2;
		if (arr[mid]== b){System.out.println(mid+1);}
		elseif(arr[mid]<b){binarysearch}
		return ;}
	
	
