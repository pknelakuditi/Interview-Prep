import java.util.Scanner;


public class SherlockBeast {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t>0){
			decent(sc.nextInt());
			t--;
		}
	}

	private static void decent(int n) {
	 if(n==1){
		 System.out.println(-1);
	 }else{
		 int x,y;
		 
		 while(x>0){
			 System.out.print(5);
			 x--;
		 }
		 while(y>0){
			 System.out.print(3);
			 y--;
		 }
		 
		 
	 }
	}

}
