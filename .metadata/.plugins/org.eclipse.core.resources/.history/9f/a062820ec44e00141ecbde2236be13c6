package algos.warmup;

import java.util.Scanner;

public class GemStone {

	public static void main(String[] args) {
		// int[] bool = new int['z' - 'a'];
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		sc.nextLine();
		int min = 0;
		int flag = 0;
		String[] str = new String[n];
		for (int i = 0; i < n; i++) {
			str[i] = sc.nextLine();
			if (str[i].length() < min) {
				min = str[i].length();
				flag = i;
			}
		}
		unique(str, flag);
		sc.close();
	}

	private static void unique(String[] str, int flag) {
        int flag1=0;
        char t='@';
		for (int j = 0; j < str[flag].length(); j++) {
           
			for (int i = 0; i < str.length; i++) {
                if(t==str[flag].charAt(j)){
					break;
				}
				 t=str[flag].charAt(j);
				if (i != flag) {
					if(str[i].indexOf(t)==-1){
						break;
					}else{
						if(i==str.length-1){
							System.out.println(t);
                            flag1++;
						}
					}
				}
			}
			
		}
		System.out.println(flag1);
	}
}
