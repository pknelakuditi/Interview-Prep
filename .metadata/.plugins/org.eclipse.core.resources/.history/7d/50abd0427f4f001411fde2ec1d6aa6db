import java.util.Scanner;

public class RecursiveStones {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for (int i = 0; i < t; i++) {
			stones(sc.nextInt(), sc.nextInt(), sc.nextInt(), 0);
		}

	}

	private static void stones(int n, int a, int b, int temp) {
         if(n==0){
        	System.out.println(temp);
         }else{
        	 System.out.println();
         }
	}

}
