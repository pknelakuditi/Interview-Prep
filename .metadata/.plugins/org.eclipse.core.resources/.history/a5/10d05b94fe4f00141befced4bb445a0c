import java.util.Scanner;

public class SherlockAndGcd {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int testCases = sc.nextInt();
		while (testCases > 0) {
			int n = sc.nextInt();
			int[] a = new int[n];
			for (int i = 0; i < n; i++) {
				a[i] = sc.nextInt();
			}
			subsetGcd(a);
			testCases--;
		}
	}

	private static void subsetGcd(int[] a) {
		int n = a.length;
		for (int i = 0; i < n; i++) {
			for (int j = i; j < n; j++) {
                 if(gcd(a[i],a[j])){
                	 System.out.println("NO");
                 }
			}
			
		}

	}
}
