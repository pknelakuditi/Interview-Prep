import java.util.Scanner;


public class IsFibo {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		int testCases=sc.nextInt();
		long[] ar=new long[testCases];
		long max=0;
		for(int i=0;i<testCases;i++){
			ar[i]=sc.nextLong();
			if(ar[i]>max){
				max=ar[i];
			}
		}
	}

}
