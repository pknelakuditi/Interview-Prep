package algos.standard.sorting;

public class Search {
    private int [] a;
	private int b;
	Search(int[] a ,int b){this.a=a;this.b=b;}
	
	public void binarysearch(int[] arr,int start,int end){
		int arr_len= start-end+1;
		int mid=arr_len/2;
		if (arr[mid]== b){System.out.println(mid+1);}
		else if(arr[mid]<b){binarysearch(arr,0,mid-1);}
		else if(arr[mid]>b){binarysearch(arr,mid+1,end);}
		else if(start==end){System.out.println("No Match");}
		else { System.out.println("Algo is wrong");}
	
	}
	public static void main(String[] args) {
		

	}

}
