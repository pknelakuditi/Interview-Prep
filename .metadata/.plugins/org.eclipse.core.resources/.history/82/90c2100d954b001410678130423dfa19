import java.util.Arrays;
import java.util.LinkedList;
import java.util.Scanner;

public class Tan {

	public static void main(String[] args) {

		//int[][] points = new int[][] { { 1, 0 }, { 0, -1 }, { -1, 0 }, { 0, 1 } };
		//polarAngles(points);

		 LinkedList<int[]> head=new LinkedList<int[]>();
		  Scanner sc = new Scanner(System.in); int n = sc.nextInt();
		  int[] point = new int[2]; 
		  for (int i1 = 0; i1 < n; i1++) {
		  point[0] = sc.nextInt(); point[1] = sc.nextInt();
		  head.add(point);
		  }
		 
		 sc.close();
		 

	}

	public static void polarAngles( LinkedList<int[]> head) {
		int[][] b = new int[head.size()][2];
		for (int i = 0; i < head.size(); i++) {
			// b[i][1] = (int) Math.atan2();
			int ang = (int) Math.toDegrees(Math.atan2(a[i][1], a[i][0]));
			if (ang < 0)
				ang = ang + 360;
			b[i][1] = ang;
			b[i][0] = i;
		}

		Arrays.sort(b, new java.util.Comparator<int[]>() {
			public int compare(int[] a, int[] b1) {
				return a[1] - b1[1];
			}
		});

		for (int i1 = 0; i1 < head.size(); i1++) {
			System.out.println(a[b[i1][0]][0] + " " + a[b[i1][0]][1]);
		}

	}

}
